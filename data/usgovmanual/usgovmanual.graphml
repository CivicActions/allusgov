  <data key="d6">International Criminal Police Organization (INTERPOL)–Washington</data>
  <data key="d7">http://www.justice.gov/interpol-washington</data>
  <data key="d8">Department of Justice, Washington, DC 20530</data>
  <data key="d33">202-616-9000</data>
  <data key="d1616">202-616-8400</data>
