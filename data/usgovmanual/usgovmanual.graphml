  <data key="d530">Kristalina Georgieva</data>
  <data key="d412">MANAGING DIRECTOR / CHAIR OF THE EXECUTIVE BOARD</data>
  <data key="d48">https://www.imf.org/en/About/Senior-Officials/Bios/gita-gopinath</data>
