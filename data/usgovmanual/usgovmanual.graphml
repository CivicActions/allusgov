  <data key="d493">-----------------------------</data>
  <data key="d495">ADVISORS TO THE VICE PRESIDENT</data>
  <data key="d496">Rohini Kosoglu</data>
  <data key="d497">Domestic Policy</data>
  <data key="d498">Nancy McEldowney</data>
